<Entetevide>
    <Jumbo>
      <form class="reg-form">
            <div class="field-row">
              <label class="form-label">Role</label>
              <select class="field form-dropdown">
                 <option value="petsitter"> Pet sitter </option>
                 <option value="petowner"> Pet owner </option>
              </select>
           </div>
           <div class="field-row">
              <label class="form-label" for="firstName">First name</label>
              <input type="text" id="firstName" class="field text-field first-name-field" >

           </div>
           <div class="field-row">
              <label class="form-label cf" for="lastName">Last name</label>
              <input type="text" id="lastName" class="field text-field last-name-field" >

           </div>
           <div class="field-row">
              <label class="form-label" for="initials">Email</label>
              <input type="text" id="initials" class="field text-field initials-field">
           </div>
            <div class="field-row">
              <label class="form-label" for="email">Password</label>
              <input type="email" id="email" class="field text-field email-field" >

           </div>
           <div class="field-row">
              <label class="form-label" for="email">Password Verification</label>
              <input type="email" id="email" class="field text-field email-field" >

           </div>
           <div class="field-row">
              <label class="form-label" for="dateOfBirth">Date of birth</label>
              <input type="date" id="dateOfBirth" class="field date-field dob-field" min="1900-01-01" max="2015-09-18"  required>

           </div>
           <div class="field-row">
              <label class="form-label" for="tel">Phone number</label>
              <input type="tel" id="tel" class="field text-field tel-field" >

           </div>
           <div class="field-row">
              <label class="form-label" for="hs">Price</label>
              <input type="text" id="hs" class="field text-field hs-field" >

           </div>
           <div class="field-row">
              <label class="form-label" for="tel">Permanent Address</label>
              <input type="tel" id="tel" class="field text-field address-field" >

           </div>
           <div class="field-row">
              <label class="form-label">Category</label>
              <select class="form-dropdown field">
                 <option value="1st "> Dogs </option>
                 <option value="2nd "> Cats </option>
                 <option value="3rd "> Birds </option>
                 <option value="4th "> Snakes </option>
                 <option value="5th "> Humster </option>
              </select>
           </div>
           <div class="field-row">
              <label class="form-label">Number of Animals Accepted</label>
              <select class="form-dropdown field">
                 <option value="one"> 1 </option>
                 <option value="two"> 5+ </option>
                 <option value="three"> 10 </option>
                 <option value="four"> 10+ </option>
              </select>
           </div>
           <div class="field-row">
              <label class="form-label">Availability</label>
              <select class="form-dropdown field">
                 <option value="1st Year"> Monday </option>
                 <option value="2nd Year"> Tuesday </option>
                 <option value="3rd Year"> Wednesday</option>
                 <option value="4th Year"> Thursday </option>
                 <option value="5th Year"> Friday</option>
                 <option value="5th Year"> Saturday</option>
                 <option value="5th Year"> Sunday</option>
                 <option value="5th Year"> Weekend only</option>
                 <option value="5th Year"> Week days only</option>
              </select>
           </div>


           <div class="field-row">
              <label class="form-label"></label>
              <button class="form-button" type="submit">Register</button>
           </div>
      </form>
    </Jumbo>
</Entetevide>